[user]
	email = gleber.p@gmail.com
	name = Gleb Peregud
[push]
	default = simple
[credential]
	helper = cache
[alias]
	graph = log --graph --color --pretty=format:"%C(yellow)%H%C(green)%d%C(reset)%n%x20%cd%n%x20%cn%x20(%ce)%n%x20%s%n"
	stat = status -sb
	last = log -1 --stat
	unstage = reset HEAD --
        restore = reset --hard HEAD
	cached = diff --cached
[color]
	ui = auto
